{
    "include": [
        "./src/**/*"
    ],
    "compilerOptions": {
        "target": "es2020",
        "module": "commonjs",
        "lib": [
            "dom",
            "es2020"
        ],
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "jsx": "react-jsx"
    }
}